                                                <div class="col-md-3 mb-2">
                                                    <div class="card">
                                                        <div class="card-body">
                                                            <%= proyecto.IDProyecto %>
                                                            <h5 class="card-title"><strong>
                                                                    <%= user.Nombre %>
                                                                </strong></h5>
                                                            <p class="card-text">
                                                                <%= proyecto.BreveDescripcion %>
                                                            </p>
                                                            <a href="/secie/AgregarElemento?IDProyecto=<%= proyecto.IDProyecto %>"
                                                                class="btn btn-primary">Agregar elemento</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            <% }); %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="accordion-item">
                                <h2 class="accordion-header">
                                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                        data-bs-target="#panelsStayOpen-collapseFour" aria-expanded="false"
                                        aria-controls="panelsStayOpen-collapseFour">
                                        <h4>Cimiento Integral Empresarial</h4>
                                    </button>
                                </h2>
                                <div id="panelsStayOpen-collapseFour" class="accordion-collapse collapse">
                                    <div class="accordion-body">
                                        <div class="row">
                                            <% proyectosPorCategoria["Cimiento Integral Empresarial"].forEach(function(proyecto)
                                                { %>
                                                <div class="col-md-3 mb-2">
                                                    <div class="card">
                                                        <div class="card-body">
                                                            <%= proyecto.IDProyecto %>
                                                            <h5 class="card-title"><strong>
                                                                    <%= user.Nombre %>
