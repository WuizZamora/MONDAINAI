<!-- views/domicilios.ejs -->
<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Domicilios</title>
  <!-- Enlace al archivo CSS de Bootstrap desde CDN -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
  <div class="container text-center">
    <h1>INFORMACION DEL FIRMANTE PRINCIPAL.</h1>
    <!-- Tabla generada dinÃ¡micamente con EJS -->
    <table class="table table-striped">
      <thead>
        <tr>
          <th>ID CUENTA</th>
          <th>Domicilio Personal PF</th>
          <th>Domicilio Laboral PF</th>
          <th>Domicilio Personal PFAE</th>
          <th>Domicilio Laboral PFAE</th>
          <th>Domicilio de Entrega</th>
          <th>Domicilio Alternativo</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach(function(result) { %>
          <tr>
            <td><%= result.IDCuenta %></td>
            <td><%= result.DomicilioPersonalPF %></td>
            <td><%= result.DomicilioLaboralPF %></td>
            <td><%= result.DomicilioPersonalPFAE %></td>
            <td><%= result.DomicilioLaboralPFAE %></td>
            <td><%= result.DomicilioEntrega %></td>
            <td><%= result.DomicilioAlternativo %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</body>
</html>
